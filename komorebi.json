{
	"$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
	"app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.yaml",
	"window_hiding_behaviour": "Cloak",
	"cross_monitor_move_behaviour": "Insert",
	"default_workspace_padding": 15,
	"default_container_padding": 10,
	"border": false,
	"border_width": 0,
	"border_offset": -1,
	"theme": {
		"palette": "Base16",
		"name": "Ashes",
		"unfocused_border": "Base03",
		"bar_accent": "Base0D"
	},
	"mouse_follows_focus": false,
	"stackbar": {
		"height": 40,
		"mode": "OnStack",
		"tabs": {
			"width": 400
		}
	},
	"float_rules": [
		{
			"kind": "Title",
			"id": "WhatsApp"
		}
	],
	"monitors": [
		{
			"workspaces": [
				{
					"name": "General",
					"layout": "Columns"
				},
				{
					"name": "Visual Studio Code",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "Code.exe"
						}
					]
				},
				{
					"name": "Arc Browser",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "Arc.exe"
						}
					]
				},
				{
					"name": "Discord",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "Discord.exe"
						}
					]
				},
				{
					"name": "Termius",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "Termius.exe"
						}
					]
				},
				{
					"name": "Ableton",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "Ableton Live 11 Suite.exe"
						}
					]
				},
				{
					"name": "League of Legends",
					"layout": "Columns",
					"initial_workspace_rules": [
						{
							"kind": "Exe",
							"id": "LeagueClientUx"
						}
					]
				}
			]
		}
	]
}
